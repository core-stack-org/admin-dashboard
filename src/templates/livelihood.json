            {
              "type": "radiogroup",
              "name": "kitchen_gardens-gender_kitchen_gardens",
              "visibleIf": "{kitchen_gardens-demand_type_kitchen_garden} = 'Individual_demand'",
              "title": "Gender\n",
              "isRequired": true,
              "choices": [
                {
                  "value": "male",
                  "text": "Male"
                },
                {
                  "value": "female",
                  "text": "Female"
                }
              ]
            },
            {
              "type": "text",
              "name": "kitchen_gardens-ben_father_kitchen_gardens",
              "visibleIf": "{kitchen_gardens-demand_type_kitchen_garden} = 'Individual_demand'",
