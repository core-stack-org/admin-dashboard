        {
          "type": "text",
          "name": "plan_id",
          "title": "Plan ID",
          "readOnly": true
        },
        {
          "type": "text",
          "name": "plan_name",
          "title": "Plan name",
          "readOnly": true
        },
        {
          "type": "text",
          "name": "user_latlon",
          "title": "Current location of the user",
          "readOnly": true
        }
      ]
