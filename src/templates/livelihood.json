            {
              "type": "radiogroup",
              "name": "fisheries-gender_fisheries",
              "visibleIf": "{fisheries-demand_type_fisheries} = 'Individual_demand'",
              "title": "Gender",
              "isRequired": true,
              "choices": [
                {
                  "value": "male",
                  "text": "Male"
                },
                {
                  "value": "female",
                  "text": "Female"
                }
              ]
            },
            {
              "type": "text",
              "name": "fisheries-ben_father_fisheries",
              "visibleIf": "{fisheries-demand_type_fisheries} = 'Individual_demand'",
              "title": "Beneficiary's father's name",
              "isRequired": true
            },
            {
              "type": "radiogroup",
              "name": "fisheries-demands_promoting_fisheries",
